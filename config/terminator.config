[global_config]
  focus = mouse
  handle_size = 0
  inactive_color_offset = 0.95145631068
  title_font = Fira Code 10.5
  title_inactive_bg_color = "#4C566A"
  title_inactive_fg_color = "#D8DEE9"
  title_receive_bg_color = "#8FBCBB"
  title_receive_fg_color = "#2E3440"
  title_transmit_bg_color = "#88C0D0"
  title_transmit_fg_color = "#2E3440"
  title_use_system_font = False
[keybindings]
  go_down = <Alt>j
  go_left = <Alt>h
  go_right = <Alt>l
  go_up = <Alt>k
  resize_down = <Shift><Alt>j
  resize_left = <Shift><Alt>h
  resize_right = <Shift><Alt>l
  resize_up = <Shift><Alt>k
  rotate_ccw = <Shift><Alt>r
  search = <Shift><Alt>f
  split_horiz = <Shift><Alt>o
  split_vert = <Shift><Alt>i
  layout_launcher = <Shift><Alt>l
  rotate_cw = <Alt>r
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      profile = default
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
[profiles]
  [[nord]]
    background_color = "#2e3440"
    background_darkness = 1.0
    background_type = transparent
    copy_on_selection = True
    cursor_color = "#D8DEE9"
    font = Fira Code weight=453 10.5
    foreground_color = "#d8dee9"
    icon_bell = False
    palette = "#3b4252:#bf616a:#a3be8c:#ebcb8b:#81a1c1:#b48ead:#88c0d0:#e5e9f0:#4c566a:#bf616a:#a3be8c:#ebcb8b:#81a1c1:#b48ead:#8fbcbb:#eceff4"
    scrollback_infinite = True
    scrollbar_position = hidden
    show_titlebar = False
    use_system_font = False
