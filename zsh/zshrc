if [[ -d "$ANTIGEN" ]]
then
    source "$ANTIGEN/antigen.zsh"
    antigen init "$DOTFILES/zsh/antigenrc"
else
    echo "Anttigen not found :s"
fi

# Load custom functions and aliases
for config ($DOTFILES/*/*.zsh) source $config

