# Load Oh My Zsh and plugins
test -f "$HOME/.zplug/init.zsh" && source $DOTFILES/zsh/zplugrc

# Load the prompt
[[ "$(command -v starship)" ]] && eval "$(starship init zsh)"

# Load custom functions and aliases
for config ($DOTFILES/*/init.zsh) source $config

