#%Module1.0######################################################################
##
## dot modulefile
##
proc ModulesHelp { } {
    puts stderr "Load Texlive distribution"
}

module-whatis   "Loads Texlive distribution"

set version 2019
set prefix /usr/local/texlive/2019

prepend-path PATH $prefix/bin/x86_64-linux/
prepend-path MANPATH $prefix/texmf-dist/doc/man/
prepend-path INFOPATH $prefix/texmf-ist/doc/info/
