#%Module1.0######################################################################
##
## dot modulefile
##
proc ModulesHelp { } {
    puts stderr "Load Python 3.7.5"
}

module-whatis   "Loads Python 3.7.5"

set version 3.7.5
set prefix $env(HOME)/.local/install/cpython-$version

prepend-path PATH $prefix/bin
prepend-path LD_LIBRARY_PATH $prefix/lib
prepend-path C_INCLUDE_PATH $prefix/include
prepend-path MANPATH $prefix/share
